package com.day16;

import java.util.Random;
import java.util.Scanner;

class MyRandom extends Thread{
	
	Scanner sc = new Scanner(System.in);
	
	private int inwon;
	
	private int []type;
	
	private static String[] name =
		{"전상원","박  범","김찬주","진현주","남승은",
		"박가윤","서동혁","양세리","김창훈","이주현",
		"왕혜민","이종원","장하은","김성재","채현우",
		"이윤재","전성덕","이나영","배승환","이한솔"};
	
	public MyRandom(String[] name) {
		
		this.name = name;
	
	}
	
	@Override
	public void run() {
	
		for(int i = 0; i <= 10; i++) {
			System.out.println(".");
		
			try {
				Thread.sleep(100);
			}catch (Exception e) {
				
			}
		}
		do {
			System.out.print("늦게까지 공부할 사람 인원수?: ");
			inwon = sc.nextInt();
			
		}while(inwon > name.length || inwon < 1);
		
		
			//Random rd = new Random();
			while(type[] < inwon) {
				
				type[] = rd.toString();
				
				for(int i = 0; i < name.length; i ++) {
					
					name[i] = sc.next();
				}
			}	
	}
	
	
}
