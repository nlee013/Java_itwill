package com.day11;

//Singleton: static?

class Sing{
	
	private static Sing ob;//static 들어간 변수는 class 변수
	
	public static Sing getInstance() {
		
		if(ob == null)
			ob = new Sing();
		return ob;
	}
	
	
}

public class Test57 {

	public static void main(String[] args) {

		Sing ob1 = Sing.getInstance();
		Sing ob2 = Sing.getInstance();
		
		if(ob1 == ob2)
			System.out.println("동일한 객체...");
	}

}
