package com.day10;

class Test{
	
	private String title;
	protected double area;
	
	public Test() {} //扁夯 积己磊
	
	public Test(String title) {//overloading 积己磊
		this.title = title;
	}
	
	public void write() {//老馆 method
		System.out.println(title + ":" + area);
	}
}

public class Test47 {

	public static void main(String[] args) {
		
	}

}
